#include <stdio.h>
#include "platform.h"

#define MAX_LENGTH 10

int main()
{
    init_platform();

    for (int i = 1; i < MAX_LENGTH; i++)
    {
    	xil_printf("%d\n", fibo(i));
    }


    cleanup_platform();

	return 0;
}

int fibo (int val)
{
	if (val == 0 || val == 1)
		return val;
	else
		return (fibo(val-1) + fibo(val-2));
}
