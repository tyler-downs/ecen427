#include <stdio.h>
#include "platform.h"

#define MAX_LENGTH 10

int fibo (int val)
{
	if (val == 0 || val == 1)
		return val;
	else
		return (fibo(val-1) + fibo(val-2));
}

int main()
{
    init_platform();
    int i;
    for (i = 0; i < MAX_LENGTH; i++)
    {
    	xil_printf("%d\n\r", fibo(i));
    }

    cleanup_platform();

	return 0;
}


